{
    "title": "function 使用教程",
    "description": "记录 function 的配置和一些插件的使用",
    "author": "chenjing",
    "output.name": "site",
    "language": "zh-hans",
    "root": ".",
    "structure": {
        "readme": "introduction.md"
    },
    "plugins": [
        "edit-link",
        "-highlight", "prism-ext",
        "github",
        "anker-enable",
        "hints-istex"
    ],
    "pluginsConfig": {
        "hints": {
            "info": "fa fa-info-circle",
            "success": "fa fa-check-circle",
            "danger": "fa fa-exclamation-triangle",
            "warning": "fa fa-exclamation-circle"
        },
        "edit-link": {
            "base": "https://github.com/cjcj5438/function/master",
            "label": "Edit This Page"
        },
        "github": {
            "url": "https://github.com/cjcj5438/function/"
        },
        "prism": {
            "plugins": ["line-numbers"],
            "cssClasses": "line-numbers"
        }
    }
}
